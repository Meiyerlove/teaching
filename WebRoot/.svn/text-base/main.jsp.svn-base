<%@ page contentType="text/html; charset=gbk" %>
<%@ taglib prefix="s" uri="/struts-tags" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>金博瑞会员消费管理系统</title>
	<link href="css/mymain.css" rel="stylesheet" type="text/css" />
</head>
<body>
 <form action="teamLeaderList.action" method="post">
 <s:if test="hasActionErrors()||hasActionMessages()">
	<div id="div-error">
	  <table>
	    <tr>
	      <th>提示信息 </th>
	    </tr>
	    <tr>
	      <td><span class="redfont"><s:fielderror /><s:actionerror /><s:actionmessage/></span></td>
	    </tr>
	  </table>
	</div>	
 </s:if>
  <div id="page">
	<table>
		<tr>
			<td width="100%"><br></br></td>
		</tr>
		<tr>
			<td width="100%" align="center"><marquee>欢迎进入金博瑞会员消费管理系统操作平台！</marquee></td>
		</tr>
		</table>
	<table>
		<tr><td width="100%" align="center">处理特殊刷卡记录 <s:property value="user"/></td></tr>
	</table>
 </div>
</form>
</body>
</html>


