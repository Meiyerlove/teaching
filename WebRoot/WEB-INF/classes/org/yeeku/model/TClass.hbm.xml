<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.yeeku.model">
	<class name="TClass"	table="tclass">
		<id	name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="cid" type="java.lang.String">
            <column name="cid" length="45"/>
        </property>
        <property name="name" type="java.lang.String">
            <column name="tname" length="200"/>
        </property>
        <property name="assistantid" type="java.lang.Integer">
            <column name="assistantid" length="45"/>
        </property>
        <property name="majorid" type="java.lang.String">
            <column name="majorid" length="45"/>
        </property>
        <property name="project" type="java.lang.String">
            <column name="project" length="45"/>
        </property>
        <property name="grade" type="java.lang.String">
            <column name="grade" length="20"/>
        </property>
	</class>	
</hibernate-mapping>