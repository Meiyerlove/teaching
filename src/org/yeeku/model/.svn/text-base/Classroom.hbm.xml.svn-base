<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.yeeku.model">
	<class name="Classroom"	table="classroom">

		<id	name="id" column="id">
			<generator class="identity"/>
		</id>
		<property name="cid" type="java.lang.String">
            <column name="cid" length="45"/>
        </property>
		<property name="roomid" type="java.lang.String">
            <column name="roomid" length="45"/>
        </property>
		<property name="roomname" type="java.lang.String">
            <column name="roomname" length="80"/>
        </property>
		<property name="campus" type="java.lang.String">
            <column name="campus" length="45"/>
        </property>
		<property name="type" type="java.lang.Integer">
            <column name="type" length="10"/>
        </property>
	</class>	
</hibernate-mapping>