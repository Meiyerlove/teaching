<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.yeeku.model">
	<class name="Goods"	table="goods">
		<id	name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="name" type="java.lang.String">
            <column name="name" length="20"/>
        </property>
        <property name="stand" type="java.lang.String">
            <column name="stand" length="20"/>
        </property>
        <property name="money" type="java.lang.Integer">
            <column name="money" length="10"/>
        </property>
        <property name="leavings" type="java.lang.Integer">
            <column name="leavings" length="10"/>
        </property>
        <property name="orders" type="java.lang.Integer">
            <column name="orders" length="10"/>
        </property>
	</class>	
</hibernate-mapping>